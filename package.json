{
  "name": "face-moderna-dr-roberio-brandao",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "build:check": "npm run build && node scripts/check-internal-links.mjs --ignore-translations",
    "check-links": "node scripts/check-internal-links.mjs",
    "check-links:strict": "node scripts/check-internal-links.mjs",
    "check-links:pt": "node scripts/check-internal-links.mjs --ignore-translations",
    "check-links:external": "node scripts/check-external-links.mjs --sample=50",
    "check-links:external-all": "node scripts/check-external-links.mjs",
    "preview": "astro preview",
    "astro": "astro",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:gemini": "node scripts/test-gemini.mjs",
    "lint": "eslint src --max-warnings 0",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,astro,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,astro,json,md}\"",
    "markdownlint": "npx markdownlint-cli \"**/*.md\" --ignore node_modules --ignore .git --ignore dist",
    "markdownlint:fix": "npx markdownlint-cli \"**/*.md\" --ignore node_modules --ignore .git --ignore dist --fix",
    "typecheck": "tsc --noEmit",
    "quality": "npm run typecheck && npm run lint && npm run format:check && npm run test:run",
    "image": "node generate-image.mjs",
    "images:all": "node generate-all-images.mjs",
    "images:optimize": "node scripts/optimize-blog-images.mjs",
    "pagespeed": "node scripts/pagespeed-insights.mjs"
  },
  "dependencies": {
    "@astrojs/mdx": "^4.3.13",
    "@astrojs/react": "^4.0.0",
    "@astrojs/sitemap": "^3.2.0",
    "@astrojs/tailwind": "^6.0.0",
    "@google/generative-ai": "^0.24.1",
    "@vercel/speed-insights": "^1.3.1",
    "astro": "^5.0.0",
    "dompurify": "^3.3.1",
    "dotenv": "^17.2.3",
    "esbuild": "^0.27.2",
    "isomorphic-dompurify": "^2.34.0",
    "openai": "^6.15.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^3.4.0",
    "vite": "^7.3.0",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@playform/compress": "^0.2.0",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/dompurify": "^3.0.5",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "typescript-eslint": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@eslint/js": "^9.17.0",
    "astro-eslint-parser": "^0.16.3",
    "eslint": "^9.17.0",
    "eslint-plugin-astro": "^1.2.0",
    "globals": "^15.14.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "jsdom": "^25.0.0",
    "markdownlint-cli": "^0.47.0",
    "markdownlint-cli2": "^0.20.0",
    "prettier": "^3.2.5",
    "prettier-plugin-astro": "^0.13.0",
    "sharp": "^0.34.5",
    "typescript": "^5.6.0",
    "vitest": "^3.2.4"
  }
}
